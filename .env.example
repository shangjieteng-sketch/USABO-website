# JWT Secret for token signing (REQUIRED) - Use a strong random string
JWT_SECRET=your_super_secret_jwt_key_change_this_in_production

# Session Secret for express-session (REQUIRED) - Use a strong random string
SESSION_SECRET=your_super_secret_session_key_change_this_in_production

# Google OAuth Configuration
# Get these from: https://console.developers.google.com/
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GOOGLE_CALLBACK_URL=https://usabo-website.vercel.app/api/auth/google/callback

# GitHub OAuth Configuration  
# Get these from: https://github.com/settings/applications/new
GITHUB_CLIENT_ID=your_github_client_id
GITHUB_CLIENT_SECRET=your_github_client_secret
GITHUB_CALLBACK_URL=https://usabo-website.vercel.app/api/auth/github/callback

# OpenAI API Key (for AI features)
OPENAI_API_KEY=your_openai_api_key

# Server Configuration
PORT=3002
NODE_ENV=development